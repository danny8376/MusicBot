{
    "defaultSeverity": "warning",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "curly": [true, "ignore-same-line"],
        "arrow-parens": [true, "ban-single-arg-parens"],
        "max-line-length": [true, 210],
        "trailing-comma": false,
        "no-console": false,
        "no-var-requires": false
    },
    "rulesDirectory": []
}